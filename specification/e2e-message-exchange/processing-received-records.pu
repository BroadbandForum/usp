@startuml
start
while (Expected ""<sequence_id>""?) is (yes)
fork
if (Has ""<payload>""?) then (yes)
:Process ""<payload>"";
endif
fork again
if (Has ""<retransmit_id>""?) then (yes)
:Resend requested record;
endif
end fork
if (Unprocessed records?) then (no)
stop
endif
endwhile (no)
if (""<sequence_id>"" < ""<expected_id>""?) then (yes)
:Gracefully ignore;
stop
else (no)
:Add to unprocessed records;
stop
@enduml
