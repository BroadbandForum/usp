<?xml version="1.0" encoding="UTF-8"?>
<dm:document
  xmlns:dm="urn:broadband-forum-org:cwmp:datamodel-1-5"
  xmlns:dmr="urn:broadband-forum-org:cwmp:datamodel-report-0-1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:broadband-forum-org:cwmp:datamodel-1-5
                        http://www.broadband-forum.org/cwmp/cwmp-datamodel-1-5.xsd
                      urn:broadband-forum-org:cwmp:datamodel-report-0-1
                        http://www.broadband-forum.org/cwmp/cwmp-datamodel-report.xsd"
  spec="urn:broadband-forum-org:example1"
  file="example1.xml">
  <description>
    DM instance that illustrates collections.
  </description>

  <bibliography>
    <reference id="REF">
      <name>Placeholder</name>
    </reference>
  </bibliography>

  <model name="Collection:1.0">
    <object name="Device." access="readOnly" minEntries="1" maxEntries="1">
      <description>
        Data model root.
      </description>
      
      <parameter name="AardvarkNumberOfEntries" access="readOnly">
        <description>{{numentries}}</description>
        <syntax><unsignedInt/></syntax>
      </parameter>
      <parameter name="LightNumberOfEntries" access="readOnly">
        <description>{{numentries}}</description>
        <syntax><unsignedInt/></syntax>
      </parameter>
      <parameter name="SwitchNumberOfEntries" access="readOnly">
        <description>{{numentries}}</description>
        <syntax><unsignedInt/></syntax>
      </parameter>
    </object>
    
    <object name="Device.Collections." access="readOnly" minEntries="1" maxEntries="1">
      <description>
        A collection of collections.
      </description>

      <parameter name="SimpleCollection" access="readWrite">
        <description>
          Simple collection of references to arbitrary objects.
          {{list}} {{reference}}
        </description>
        <syntax>
          <list/>
          <string>
            <pathRef refType="strong" targetType="object"/>
          </string>
        </syntax>
      </parameter>
      
      <parameter name="LightingCollection" access="readWrite">
        <description>
          More restricted collection of references to Light and Switch instances.
          {{list}} {{reference}}
        </description>
        <syntax>
          <string>
            <pathRef refType="strong" targetType="row" targetParent=".Light. .Switch."/>
          </string>
        </syntax>
      </parameter>

      <parameter name="SimpleCollectionTableNumberOfEntries" access="readOnly">
        <description>{{numentries}}</description>
        <syntax><unsignedInt/></syntax>
      </parameter>
      <parameter name="LightingCollectionTableNumberOfEntries" access="readOnly">
        <description>{{numentries}}</description>
        <syntax><unsignedInt/></syntax>
      </parameter>
    </object>
    
    <object name="Device.Collections.SimpleCollectionTable.{i}." access="readWrite" minEntries="0" maxEntries="unbounded"
      numEntriesParameter="SimpleCollectionTableNumberOfEntries">
      <description>
        Simple collection that's a table so additional local metadata can be added.
      </description>
      
      <parameter name="Name" access="readWrite">
        <description>
          Local collection member name.
        </description>
        <syntax>
          <string/>
        </syntax>
      </parameter>
      
      <parameter name="Reference" access="readWrite">
        <description>
          Collection member.
          {{reference}}
        </description>
        <syntax>
          <string>
            <pathRef refType="strong" targetType="object"/>
          </string>
        </syntax>
      </parameter>
    </object>
    
    <object name="Device.Collections.LightingCollectionTable.{i}." access="readWrite" minEntries="0" maxEntries="unbounded"
      numEntriesParameter="LightingCollectionTableNumberOfEntries">
      <description>
        Lighting collection that's a table so additional local metadata can be added.
      </description>
      
      <parameter name="Name" access="readWrite">
        <description>
          Local collection member name.
        </description>
        <syntax>
          <string/>
        </syntax>
      </parameter>
      
      <parameter name="Reference" access="readWrite">
        <description>
          Collection member.
          {{reference}}
        </description>
        <syntax>
          <string>
            <pathRef refType="strong" targetType="row" targetParent=".Light. .Switch."/>
          </string>
        </syntax>
      </parameter>
    </object>
    
    <object name="Device.Aardvark.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" dmr:fixedObject="true"
            numEntriesParameter="AardvarkNumberOfEntries"/>
    <object name="Device.Light.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" dmr:fixedObject="true"
            numEntriesParameter="LightNumberOfEntries"/>
    <object name="Device.Switch.{i}." access="readOnly" minEntries="0" maxEntries="unbounded" dmr:fixedObject="true"
            numEntriesParameter="SwitchNumberOfEntries"/>
    
  </model>
</dm:document>