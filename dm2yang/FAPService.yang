          }

          list Cell {
            config false;
            description
              "Table indicating the components of the discovered
               cells.";

            container RF {
              description
                "RF REM results for each detected cell.";
              leaf Band {
                type int32;
                description
                  "CDMA band for the reported cell.";
              }
              leaf RFCNFL {
                type uint32 {
                  range "0..2016";
                }
                description
                  "The Radio Frequency Channel Number (RFCN) in the
                   FL direction for the reported cell.";
              }
              leaf Io {
                type int32 {
                  range "-105..-25";
                }
                description
