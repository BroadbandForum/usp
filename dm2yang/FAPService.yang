                type int32;
                description
                  "System identification.  Refer to
                   {{bibref|3GPP2-C.S0005}}.";
              }
              leaf NID {
                type int32;
                description
                  "Network identification.  Refer to
                   {{bibref|3GPP2-C.S0005}}.";
              }
              leaf REGZONE {
                type int32;
                description
                  "Registration Zone.  Refer to
                   {{bibref|3GPP2-C.S0005}}.";
              }
              leaf BASELAT {
                type int32 {
                  range "-2147483648..2147483647";
                }
                description
                  "Basestation latitude.  Refer to
                   {{bibref|3GPP2-C.S0005}}.";
              }
              leaf BASELONG {
                type int32 {
                  range "-2147483648..2147483647";
                }
                description
                  "Basestation longtitude.  Refer to
                   {{bibref|3GPP2-C.S0005}}.";
